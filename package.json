{
  "name": "velobar-corporate-site",
  "version": "1.0.0",
  "description": "Velobar Corporate Site - Premium mobile cocktailbar and event catering for MÃ¼nchen and Coburg (DE/EN)",
  "main": "src/main.tsx",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:force",
    "ssg:build": "vite build",
    "build:local": "npm run optimize:gallery && vite build",
    "build:force": "echo 'Starting build...' && SKIP_VERIFY=true SKIP_IMAGE_OPTIMIZATION=true vite build",
    "preview": "vite preview",
    "start": "vite dev",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test --reporter=list",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:p0": "playwright test tests/p0 --reporter=list",
    "test:p0:headed": "playwright test tests/p0 --headed",
    "test:p0:report": "playwright test tests/p0 && playwright show-report tests/p0/report",
    "test:visual": "playwright test tests/p0/visual-regression.spec.ts",
    "test:visual:update": "playwright test tests/p0/visual-regression.spec.ts --update-snapshots",
    "test:integration": "tsx src/services/integrationTesting.ts",
    "test:integration:watch": "tsx watch src/services/integrationTesting.ts",
    "test:security": "bash scripts/security-test-production.sh ${STAGING_URL:-localhost:4173}",
    "test:security:staging": "bash scripts/security-test-production.sh $STAGING_URL",
    "test:security:prod": "bash scripts/security-test-production.sh $PRODUCTION_URL",
    "lint": "eslint \"**/*.{ts,tsx}\" --config eslint.config.js",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --config eslint.config.js --fix",
    "lint:colors": "sh scripts/lint-colors.sh",
    "lint:spacing": "node scripts/check-spacing.cjs",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "validate-imports": "node scripts/validate-imports.mjs",
    "preci-checks": "npm run lint && npm run validate-imports",
    "validate:full": "npm run validate && npm test -- --run",
    "cleanup": "npm run lint:fix && npm run format",
    "deps:check": "depcheck",
    "deps:check:ci": "depcheck --json",
    "verify-backgrounds": "bash scripts/protect-backgrounds.sh",
    "verify-images": "node scripts/verify-images.cjs",
    "verify:apis": "tsx -e \"import('./src/services/integrationTesting.ts').then(m => m.runAllTests())\"",
    "optimize-images": "node scripts/optimize-images.js",
    "optimize-images:legacy": "node scripts/optimize-images.mjs",
    "optimize:gallery": "node scripts/optimize-velo-gallery.mjs",
    "optimize:perf": "node scripts/optimize-large-images.mjs",
    "convert-to-webp": "node scripts/convert-to-webp.js",
    "gallery:generate": "node scripts/generate-gallery-index.mjs",
    "gallery:stats": "node scripts/generate-gallery-index.mjs | grep -E '(Total|Artists|Categories)'",
    "gallery:manifest": "npx tsx scripts/generate-gallery-manifest.ts",
    "gallery:rebuild": "npm run gallery:manifest && npm run optimize:gallery",
    "regenerate-gallery": "node scripts/regenerate-gallery-manifest.mjs",
    "generate:test-images": "node scripts/generate-test-images.mjs",
    "seo:sitemap": "node scripts/generate-sitemap.mjs",
    "seo:validate": "node scripts/validate-seo.mjs",
    "seo-audit": "node scripts/seo-audit.cjs",
    "analyze:bundle": "node scripts/analyze-bundle.mjs",
    "analyze:all": "npm run analyze:bundle && npm run validate:imports && npm run check:circular",
    "validate:imports": "node scripts/validate-imports.mjs",
    "check:circular": "node scripts/detect-circular-deps.mjs",
    "check:health": "node scripts/health-check.mjs",
    "perf:benchmark": "node scripts/performance-benchmark.mjs",
    "perf:benchmark:prod": "node scripts/performance-benchmark.mjs https://velo.bar",
    "qa:production": "node tools/prod_capture.mjs",
    "qa:production:report": "npm run qa:production && open docs/verification/production_smoke_check.md",
    "qa:artifacts": "open docs/verification/artifacts/",
    "env:check": "node scripts/check-env.mjs",
    "setup:env": "cp .env.example .env.local && echo 'Please configure .env.local with your API keys'",
    "health-check": "curl -f http://localhost:4173/api/health || exit 1",
    "prepare": "husky install || true",
    "prepare-deploy": "npm run optimize-images && npm run build",
    "deploy:preview": "npm run prepare-deploy && vercel",
    "deploy:production": "npm run prepare-deploy && vercel --prod",
    "deploy:staging": "npm run build && npm run test:integration",
    "lhci:collect": "lhci collect",
    "lhci:assert": "lhci assert",
    "lhci": "npm run build && lhci autorun",
    "codemod:legacy": "node codemods/dryrun-replace-3.cjs",
    "optimize:images:enhanced": "node scripts/optimize-images-enhanced.mjs",
    "optimize:images:gallery": "node scripts/optimize-images-enhanced.mjs 'public/Velo Gallery'"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "react",
    "template",
    "atomic-design",
    "typescript",
    "vite",
    "tailwind",
    "content-agnostic"
  ],
  "author": "Velobar Event Catering",
  "license": "MIT",
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@netlify/functions": "^2.8.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-aria/focus": "^3.21.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/axios": "^0.9.36",
    "@vercel/blob": "^2.0.0",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.18.2",
    "gsap": "^3.14.2",
    "lucide-react": "^0.487.0",
    "nodemailer": "^7.0.12",
    "react": "18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.3.8",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.66.0",
    "react-router-dom": "^7.9.4",
    "resend": "^4.0.0",
    "scheduler": "0.23.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "terser": "^5.44.0",
    "tinyglobby": "^0.2.15",
    "uuid": "^13.0.0",
    "validator": "^13.15.23",
    "vite-plugin-sitemap": "^0.8.2",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.13",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.56.1",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.8.1",
    "@types/nodemailer": "^7.0.5",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vercel/node": "^5.5.16",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^4.0.8",
    "detect-libc": "^2.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "glob": "^11.1.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sharp": "^0.34.5",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.0.0",
    "vite": "^7.1.12",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.6"
  }
}
