{
  "ignores": [
    "@types/*",
    "eslint-*",
    "prettier",
    "stylelint",
    "@testing-library/*",
    "vitest",
    "@vitest/*",
    "playwright",
    "@playwright/*",
    "vite",
    "postcss",
    "tailwindcss",
    "@tailwindcss/*",
    "typescript",
    "sharp",
    "@biomejs/biome",
    "@stylelint/postcss-css-in-js",
    "prettier-plugin-tailwindcss",
    "stylelint-config-standard",
    "svgo"
  ],
  "skip-missing": false,
  "ignore-patterns": [
    "dist",
    "node_modules",
    "coverage",
    ".git"
  ],
  "parsers": {
    "*.js": "es6",
    "*.jsx": "jsx",
    "*.ts": "typescript",
    "*.tsx": "tsx"
  },
  "detectors": [
    "depcheck-detector-require",
    "depcheck-detector-import",
    "depcheck-detector-export"
  ],
  "specials": [
    "bin",
    "eslint",
    "husky",
    "lint-staged",
    "prettier",
    "stylelint"
  ]
}
